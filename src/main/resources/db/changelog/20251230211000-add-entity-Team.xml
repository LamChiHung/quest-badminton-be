<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
      http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.23.xsd"
>
    <changeSet id="20251230211000-01" author="hunglc">
        <createTable tableName="t_teams">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(500)">
                <constraints nullable="false"/>
            </column>
            <column name="number" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="fk_tour_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="20251230211000-02" author="hunglc">
        <addForeignKeyConstraint baseTableName="t_teams"
                                 baseColumnNames="fk_tour_id"
                                 constraintName="fk_t_tours_id"
                                 referencedTableName="t_tours"
                                 referencedColumnNames="id"/>
    </changeSet>
    <changeSet id="20251230211000-03" author="hunglc">
        <createIndex tableName="t_teams" indexName="idx_t_teams_fk_tour_id">
            <column name="fk_tour_id"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>